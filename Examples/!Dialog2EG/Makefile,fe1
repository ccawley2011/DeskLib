#

CC		=	cc
#LINK		=	DRLink -debimage -aif
LINK		=	Link

CCFlags		=	-fahi -c $(CCExtra) -depend !Depend -throwback -I,C:
LinkFlags	=	$(LinkExtra)



Libraries	=					\
			DeskLib:o.Debug.pipetype	\
			DeskLib:o.DeskLib		\
			C:o.Stubs

Target		=	!RunImage

ObjectFiles	=	o.Main		



# path(s) to source files:
VPATH = @.^ @


# Don't alter anything below this...

.SUFFIXES:	.o .c

$(Target):	$(ObjectFiles) $(Libraries)
	$(LINK) $(LinkFlags) -o $@ $(ObjectFiles) $(Libraries)

.c.o:
	$(CC) $(ccFlags) -o $@ $<

