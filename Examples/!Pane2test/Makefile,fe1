# Project:   Pane2tests

.SUFFIXES:    .c .o
CC            = cc
CCFlags       = -c -fussy -throwback -IC: -Ff
LinkFlags     = -o $@
SqueezeFlags  = -f -v

Files         = o.Pane2test o.PaneTests

Libs          = DeskLib:o.DeskLib C:o.Stubs


!RunImage:    $(Files) 
              link $(LinkFlags) $(Files) $(Libs)
              squeeze $(SqueezeFlags) !RunImage

.c.o:;        $(CC) $(CCflags) $< -o $@
